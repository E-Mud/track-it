
whiteframe(level = 2)
  if level == 1
    box-shadow: 0 0.1rem 0.3rem 0 rgba(0,0,0,.2),0 0.1rem 0.1rem 0 rgba(0,0,0,.14),0 0.2rem 0.1rem -0.1rem rgba(0,0,0,.12)
  else if level == 2
    box-shadow: 0 0.1rem 0.5rem 0 rgba(0,0,0,.2),0 0.2rem 0.2rem 0 rgba(0,0,0,.14),0 0.3rem 0.1rem -0.2rem rgba(0,0,0,.12)
  else
    box-shadow: 0 2px 4px -1px rgba(0,0,0,.2),0 4px 5px 0 rgba(0,0,0,.14),0 1px 10px 0 rgba(0,0,0,.12)
